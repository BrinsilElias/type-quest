---
import Layout from '../layouts/Layout.astro';
import TestSettings from '../components/testSettings.astro';
---

<Layout title="TypeQuest">
  <main>
    <article class="region prose wrapper">
      <TestSettings />
      <p class="wordlist"></p>
      <input
        id="keyboard-input"
        type="text"
        placeholder="Start typing"
        autocomplete="off"
      />
    </article>
  </main>
</Layout>
